<div class="notify-push-inner" role="region" aria-label="{{_accessibility._ariaLabels.feedbackPopUp}}">
	<div class="notify-push-title">
		<div class="notify-push-title-inner">
			{{{title}}}
		</div>
	</div>
	<div class="notify-push-body">
		<div class="notify-push-body-inner">
			{{{body}}}
		</div>
	</div>
</div>
<a href="#" class="notify-push-close" role="button" aria-label="{{_accessibility._ariaLabels.closePopup}}">
	<span class="icon icon-cross"></span>
</a>